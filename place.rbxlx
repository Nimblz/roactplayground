								<ProtectedString name="Source"><![CDATA[local ReplicatedStorage = game:GetService("ReplicatedStorage")

local common = ReplicatedStorage:WaitForChild("common")
local lib = ReplicatedStorage:WaitForChild("lib")
local util = common:WaitForChild("util")
local uiComponents = script:FindFirstAncestor("uiComponents")

local Dictionary = require(util.Dictionary)

local Roact = require(lib:WaitForChild("Roact"))

local AutoSizedTextElement = require(uiComponents.AutoSizedTextElement)

local TextLabelList = Roact.Component:extend("TextLabelList")

function TextLabelList:init(initialProps)
    self.contentSize, self.updateContentSize = Roact.createBinding(UDim2.new(0,0,0,0))
    self.layoutRef = Roact.createRef()
end

function TextLabelList:render()
    local textElements = {}

    for idx,text in ipairs(self.props.strings) do
        textElements[idx] = Roact.createElement(AutoSizedTextElement, Dictionary.join({
            type = self.props.elementType or "TextLabel",
            Text = text,
            Font = Enum.Font.GothamBlack,
            TextSize = 32,
            BackgroundColor3 = Color3.fromRGB(255,255,255),
            BorderSizePixel = 0,
            LayoutOrder = idx,
            TextXAlignment = Enum.TextXAlignment.Left,

            ClearTextOnFocus = false,
        }, self.props.elementProps or {}))
    end

    local layout = Roact.createElement("UIListLayout", {
        SortOrder = Enum.SortOrder.LayoutOrder,
        Padding = UDim.new(0,4),
        [Roact.Ref] = self.layoutRef,
    })

    local joinedProps = Dictionary.join(self.props, {
        elementType = Dictionary.None,
        elementProps = Dictionary.None,
        strings = Dictionary.None,
        Size = self.elementsSize
    })

    local children = Dictionary.join(textElements,{layout = layout})

    return Roact.createElement("Frame", joinedProps, children)
end

function TextLabelList:didMount()
    self:updateSizeBinding()
end

function TextLabelList:didUpdate()
    self:updateSizeBinding()
end

function TextLabelList:updateSizeBinding()
    local layout = self.layoutRef:getValue()
    if not layout then return end

	local newContentSize = UDim2.new(
		0, layout.AbsoluteContentSize.X,
		0, layout.AbsoluteContentSize.Y)

	self.updateContentSize(newContentSize)
end

return TextLabelList]]></ProtectedString>
